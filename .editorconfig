# editorconfig.org
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[**.json]
end_of_line = lf
insert_final_newline = false
charset = utf-8
trim_trailing_whitespace = true

[.gitignore]
end_of_line = lf
insert_final_newline = false
charset = utf-8
trim_trailing_whitespace = true

[README.md]
end_of_line = lf
insert_final_newline = false
charset = utf-8
trim_trailing_whitespace = true
